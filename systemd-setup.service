[Unit]
Description=System Setup Wizard
DefaultDependencies=no
Conflicts=shutdown.target
After=basic.target
Before=shutdown.target
ConditionPathIsReadWrite=/etc

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/bin/system-setup --first-start
StandardOutput=tty
StandardInput=tty
StandardError=tty

[Install]
WantedBy=sysinit.target
